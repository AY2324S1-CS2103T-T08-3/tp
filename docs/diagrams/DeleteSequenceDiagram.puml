@startuml
!include style.puml
skinparam ArrowFontStyle plain

box Logic LOGIC_COLOR_T1
participant ":LogicManager" as LogicManager LOGIC_COLOR
participant ":TaskHubParser" as TaskHubParser LOGIC_COLOR
participant ":DeleteEmployeeComandParser" as DeleteEmployeeComandParser LOGIC_COLOR
participant "d:DeleteEmployeeComand" as DeleteEmployeeComand LOGIC_COLOR
participant ":CommandResult" as CommandResult LOGIC_COLOR
end box

box Model MODEL_COLOR_T1
participant ":Model" as Model MODEL_COLOR
end box

[-> LogicManager : execute("deleteE 1")
activate LogicManager

LogicManager -> TaskHubParser : parseCommand("deleteE 1")
activate TaskHubParser

create DeleteEmployeeComandParser
TaskHubParser -> DeleteEmployeeComandParser
activate DeleteEmployeeComandParser

DeleteEmployeeComandParser --> TaskHubParser
deactivate DeleteEmployeeComandParser

TaskHubParser -> DeleteEmployeeComandParser : parse("1")
activate DeleteEmployeeComandParser

create DeleteEmployeeComand
DeleteEmployeeComandParser -> DeleteEmployeeComand
activate DeleteEmployeeComand

DeleteEmployeeComand --> DeleteEmployeeComandParser : d
deactivate DeleteEmployeeComand

DeleteEmployeeComandParser --> TaskHubParser : d
deactivate DeleteEmployeeComandParser
'Hidden arrow to position the destroy marker below the end of the activation bar.
DeleteEmployeeComandParser -[hidden]-> TaskHubParser
destroy DeleteEmployeeComandParser

TaskHubParser --> LogicManager : d
deactivate TaskHubParser

LogicManager -> DeleteEmployeeComand : execute()
activate DeleteEmployeeComand

DeleteEmployeeComand -> Model : deleteEmployee(1)
activate Model

Model --> DeleteEmployeeComand
deactivate Model

create CommandResult
DeleteEmployeeComand -> CommandResult
activate CommandResult

CommandResult --> DeleteEmployeeComand
deactivate CommandResult

DeleteEmployeeComand --> LogicManager : result
deactivate DeleteEmployeeComand

[<--LogicManager
deactivate LogicManager
@enduml
